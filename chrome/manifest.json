{
    "name": "Gfycat Hoverzoom",
    "short_name": "Gfycat Hover",
    "version": "2",
    "manifest_version": 2,
    "minimum_chrome_version": "26.0",
    "description": "When you hover with your mouse over a gfycat link you'll see the video popup and play. Much like HoverZoom for a gif.",
    "background": {
        "scripts": []
    },
    "homepage_url": "http://gfycat.com",
    "content_scripts": [{
        "matches": [
            "http://reddit.com/*",
            "https://reddit.com/*",
            "http://*.reddit.com/*",
            "https://*.reddit.com/*"
        ],
        "js": [
            "jquery-2.0.3.js",
            "gfycat_hoverzoom.user.js"
        ],
        "css": [
        ]
    }],
    "web_accessible_resources": [
    ],
    "icons": {
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "permissions": [
        "cookies",
        "tabs",
        "history",
        "http://reddit.com/*",
        "https://reddit.com/*",
        "http://*.reddit.com/*",
        "https://*.reddit.com/*",
        "http://redditgifts.com/*",
        "http://api.twitter.com/*"
    ],
    "optional_permissions": [
        "https://api.twitter.com/*"
    ]
}